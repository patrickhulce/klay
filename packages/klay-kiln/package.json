{
  "name": "klay-kiln",
  "version": "0.0.0",
  "description": "Library for using klay models together and generating useful objects with them.",
  "main": "./lib/kiln.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "lint",
    "test:unit": "mocha --reporter spec --require test/bootstrap 'test/**/*.test.js'",
    "test:coverage": "nyc npm run test:unit",
    "test:watch": "mocha --watch --reporter dot --require test/bootstrap 'test/**/*.test.js'",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickhulce/klay-kiln.git"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickhulce/klay-kiln/issues"
  },
  "homepage": "https://github.com/patrickhulce/klay-kiln#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "@patrickhulce/lint": "^2.0.2",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "mocha": "^3.0.2",
    "nyc": "^11.4.1",
    "semantic-release": "^11.2.0",
    "sinon": "^1.17.5"
  }
}

{
  "name": "klay-db",
  "version": "0.0.0",
  "description": "Extensions for klay to add generic information typically used for database interactions.",
  "main": "./lib/extension.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "lint",
    "test:unit": "mocha --reporter spec --require test/bootstrap 'test/**/*.test.js'",
    "test:coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --require test/bootstrap 'test/**/*.test.js'",
    "test:watch": "mocha --watch --reporter dot --require test/bootstrap 'test/**/*.test.js'",
    "build:aliases": "generate-export-aliases",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickhulce/klay-db.git"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickhulce/klay-db/issues"
  },
  "homepage": "https://github.com/patrickhulce/klay-db#readme",
  "config": {
    "exportAliases": {
      "helpers": "./lib/helpers/index.js",
      "options": "./lib/Options.js"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "eslint": {
      "globals": [
        "defineTest"
      ]
    }
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "@patrickhulce/lint": "^2.0.2",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "generate-export-aliases": "^1.0.0",
    "istanbul": "^0.4.5",
    "klay": "^1.2.0",
    "mocha": "^3.0.2",
    "semantic-release": "^11.2.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "peerDependencies": {
    "klay": "^1.2.0"
  }
}
